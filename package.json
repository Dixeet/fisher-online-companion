{
  "name": "fisher-online-companion",
  "version": "0.1.0",
  "description": "FOC - Fisher Online Companion",
  "main": "parsers/parse.js",
  "type": "module",
  "scripts": {
    "cleanup": "find parsers/.tmp/ ! -name '.gitkeep' -type f -exec rm -rf {} +",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore\"",
    "prettier": "prettier . --write --ignore-path ./package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dixeet/fisher-online-companion.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dixeet/fisher-online-companion/issues"
  },
  "homepage": "https://github.com/Dixeet/fisher-online-companion#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2022": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ]
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "command-line-args": "^5.2.1",
    "n-readlines": "^1.0.1",
    "nodemon": "^3.0.1",
    "xml2js": "^0.6.2"
  }
}
