{
  "name": "fisher-online-companion",
  "version": "0.1.0",
  "description": "FOC - Fisher Online Companion",
  "main": "parsers/parse.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean:vite": "rm -rf .cache/* components.d.ts",
    "clean:parse": "rm -rf public/data/*",
    "clean:images": "rm -rf src/assets/images/maps/* src/assets/images/fishes/* src/assets/images/weapons/*",
    "parse": "node parsers/parse.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore\"",
    "prettier": "prettier . --write --ignore-path ./package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dixeet/fisher-online-companion.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dixeet/fisher-online-companion/issues"
  },
  "homepage": "https://github.com/Dixeet/fisher-online-companion#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2022": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ]
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@iconify-json/game-icons": "^1.1.3",
    "@iconify-json/mdi": "^1.1.54",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.8"
  },
  "dependencies": {
    "command-line-args": "^5.2.1",
    "command-line-usage": "^7.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "n-readlines": "^1.0.1",
    "nodemon": "^3.0.1",
    "sass": "^1.68.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vuetify": "^3.3.17",
    "xml2js": "^0.6.2"
  }
}
